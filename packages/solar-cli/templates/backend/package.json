{
  "name": "$projectName$",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "main": "www/index.js",
  "workspace": true,
  "workspaces": [
    "src/api/*",
    "src/web/*"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node ./src/api/$projectName$-api/index.ts",
    "build": "rm -rf ./dist &&  npm run build:api && npm run build:web",
    "build:web":"cross-env NODE_ENV=production webpack --config ./build/webpack.js --color",
    "build:api":"tsc -p src/api --outDir dist/src/api --skipLibCheck --skipDefaultLibCheck"
  },
  "keywords": [
    "$projectName$"
  ],
  "peerDependencies": {
    "sequelize": "^5.x",
    "sequelize-typescript": "^1.x",
    "node-web-mvc": "^2.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env":"^7.20.2",
    "@babel/preset-react":"^7.18.6",
    "@babel/preset-typescript":"^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@babel/runtime": "^7.20.7",
    "autoprefixer": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "solar-types": "^1.1.1",
    "solar-proxy": "^2.0.0",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-middleware": "^3.7.2",
    "@types/webpack-hot-middleware": "^2.25.3",
    "@types/webpack-env": "^1.16.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-plugin-import": "^1.13.5",
    "metro-react-native-babel-preset": "^0.64.0",
    "clean-webpack-plugin": "^3.0.0",
    "node-web-mvc": "^2.12.3",
    "cross-env": "^7.0.2",
    "css-loader": "^6.7.1",
    "eslint": "^7.13.0",
    "eslint-config-google": "0.14.0",
    "eslint-webpack-plugin": "^2.5.4",
    "eslint-plugin-react": "^7.21.5",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^2.6.0",
    "react-refresh": "^0.14.0",
    "copy-webpack-plugin": "^8.1.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "sass": "^1.29.0",
    "open": "7.3.0",
    "postcss-loader": "^6.2.1",
    "sass-loader": "^13.0.0",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-middleware": "^5.3.1",
    "webpack-hot-middleware": "^2.25.0",
    "@types/node": "^14.0.26",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "less": "^4.1.1",
    "less-loader": "^10.2.0"
  },
  "dependencies": {
    "@loadable/component": "^5.7.0",
    "@rematch/core": "^2.2.0",
    "antd": "^4.20.2",
    "core-js": "^3.23.1",
    "solar-core": "^2.3.3",
    "solar-pc": "^3.1.45",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.2.0"
  },
  "$projectName$": {
    "prefix": "$projectName$",
    "service": {
      "target": "src/web/$projectName$-services",
      "servs": [
        {
          "name": "$projectName$",
          "value": "http://localhost:8080/swagger-ui/openapi.json"
        }
      ]
    },
    "admin": {
      "target": "src/web/$projectName$/pages/",
      "router": "src/web/$projectName$/router/index.tsx",
      "enums": "src/web/$projectName$-configs/constants.ts",
      "enumsUsing": "$projectName$-configs/constants",
      "store": "$projectName$-provider",
      "ui": "$projectName$-ui",
      "services": "$projectName$-services"
    }
  },
  "license": "ISC",
  "browserslist": "chrome > 65,safari > 11"
}
