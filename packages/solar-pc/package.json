{
  "name": "solar-pc",
  "version": "5.3.10",
  "description": "solar pc",
  "main": "index",
  "scripts": {
    "start": "node ./www/index.js"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "antd": "^5.2.9",
    "solar-core": "^4.2.0"
  },
  "dependencies": {
    "css": "^3.0.0",
    "prismjs": "^1.28.0",
    "memoize-one": "^5.0.5",
    "xlsx":"^0.18.5"
  },
  "devDependencies": {
    "solar-core": "*", 

    "solar-jest-mock": "^1.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "jest": {
    "testURL": "https://solar.com/",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "testPathIgnorePatterns": []
  },
  "docs": {
    "devServer": true,
    "runnerPatterns":["src/**/demo/*.tsx"],
    "runnerIndex": "__tests__/App"
  }
}